    <ui:DatabaseConnect DisplayName="Connect to database" sap:VirtualizedContainerService.HintSize="484,59" sap2010:WorkflowViewState.IdRef="DatabaseConnect_1" ProviderName="System.Data.SqlClient" ConnectionString="Data Source=KBXREPLPROD.khc.local;Initial Catalog=TOPSReporting;Integrated Security=True" DatabaseConnection="[SQL]" />
    <ui:ExecuteQuery ContinueOnError="{x:Null}" CommandType="Text" DataTable="[DataTableOutput]" DisplayName="Run query" ExistingDbConnection="[SQL]" sap:VirtualizedContainerService.HintSize="484,88" sap2010:WorkflowViewState.IdRef="ExecuteQuery_1" Sql="--&#xD;&#xA;--&#xD;&#xA;-- 10/20/22            Landon        Created&#xD;&#xA;-- 10/26/22            Landon        Added Portal Path&#xD;&#xA;-- 12/29/22            Landon         Added POD Consolidation Path logic&#xD;&#xA;-- 07/06/2023        Tiffany/Raelynn      adding code to sort by AR Pending, Build products, then delv date&#xD;&#xA;-- 08/31/2023&#x9;&#x9;&#x9;Landon&#x9;&#x9;Added Cross Server GDS loads and Union for final output &#xD;&#xA;&#xD;&#xA;&#xD;&#xA;DROP TABLE IF EXISTS #Loads&#xD;&#xA;&#xD;&#xA;select Distinct&#xD;&#xA;l.ActualCarrierOrgId AS Carrier&#xD;&#xA;,l.loadid&#xD;&#xA;,l.LoadNbr AS BOL&#xD;&#xA;,l.PkupDate&#xD;&#xA;,l.DelvDate&#xD;&#xA;,l.ActualFleetScac AS SCAC&#xD;&#xA;,l.ActualSubFleetScac AS SubScac&#xD;&#xA;,l.ownerorgid AS OwnerOrgId&#xD;&#xA;,l.ProNbr&#xD;&#xA;,o.OwnerName&#xD;&#xA;,l.ShipperPkupNbr&#xD;&#xA;INTO #Loads&#xD;&#xA;from TOPSAnalyst.dbo.POD p&#xD;&#xA;INNER JOIN TOPS.dbo.loads l WITH (NOLOCK)&#xD;&#xA;ON p.LoadNbr = l.LoadNbr&#xD;&#xA;INNER JOIN TOPS.dbo.Owner o WITH (NOLOCK)&#xD;&#xA;ON l.OwnerOrgId = o.OwnerId&#xD;&#xA;WHERE l.LoadVisibleCd NOT IN ('-15','-16','-17')&#xD;&#xA;AND l.LoadStatusColor &lt;&gt; 'RED'&#xD;&#xA;AND l.LoadId NOT LIKE'%--%'&#xD;&#xA;;&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;SELECT DISTINCT&#xD;&#xA;l.CarrierOrgId AS Carrier&#xD;&#xA;,l.loadid&#xD;&#xA;,l.LoadNbr AS BOL&#xD;&#xA;,l.PkupDate&#xD;&#xA;,l.DelvDate&#xD;&#xA;,l.FleetScac AS SCAC&#xD;&#xA;,l.SubFleetScac AS SubScac&#xD;&#xA;,l.ownerorgid AS OwnerOrgId&#xD;&#xA;,l.ProNbr&#xD;&#xA;,o.OwnerName&#xD;&#xA;,l.ShipperPkupNbr&#xD;&#xA;,l.MovementType&#xD;&#xA;INTO #LoadDoc&#xD;&#xA;FROM TOPS.dbo.LoadDocuments ld WITH(NOLOCK)&#xD;&#xA;INNER JOIN TOPS.dbo.loads l WITH (NOLOCK)&#xD;&#xA;ON ld.loadid = l.Loadid&#xD;&#xA;INNER JOIN TOPS.dbo.Owner o WITH (NOLOCK)&#xD;&#xA;ON l.OwnerOrgId = o.OwnerId&#xD;&#xA;where ld.DocType = 'PROOF OF DELIVERY'&#xD;&#xA;--AND CAST(ld.CreateDtTm AS DATE) = DATEADD(day,-1,CAST(GETDATE() AS DATE))&#xD;&#xA;;&#xD;&#xA;&#xD;&#xA;--Cross Server for Load id&#xD;&#xA;SELECT DISTINCT &#xD;&#xA;l.CarrierOrgId AS Carrier&#xD;&#xA;,ld.PropertyValue AS Loadid&#xD;&#xA;,l.LoadNbr AS BOL&#xD;&#xA;,l.PkupDate&#xD;&#xA;,l.DelvDate&#xD;&#xA;,l.FleetScac AS SCAC&#xD;&#xA;,l.SubFleetScac AS SubScac&#xD;&#xA;,l.ownerorgid AS OwnerOrgId&#xD;&#xA;,l.ProNbr&#xD;&#xA;,o.OwnerName&#xD;&#xA;,l.ShipperPkupNbr&#xD;&#xA;,l.MovementType&#xD;&#xA;INTO #LoadDocCS&#xD;&#xA;FROM TOPSAnalyst.dbo.PODCrossServerGDS ld WITH (NOLOCK)&#xD;&#xA;INNER JOIN TOPS.dbo.loads l WITH (NOLOCK)&#xD;&#xA;ON ld.PropertyValue = l.Loadid&#xD;&#xA;INNER JOIN TOPS.dbo.Owner o WITH (NOLOCK)&#xD;&#xA;ON l.OwnerOrgId = o.OwnerId&#xD;&#xA;WHERE ld.PropertyName IN ('Billing Load Id','KBX TM Mobile Load Id','Reference Load Id','Load ID')&#xD;&#xA;--AND CAST(ld.LastChgDtTm AS DATE) = DATEADD(day,-1,CAST(GETDATE() AS DATE))&#xD;&#xA;;&#xD;&#xA;&#xD;&#xA;--Cross Server for Load Nbr&#xD;&#xA;SELECT DISTINCT &#xD;&#xA;l.CarrierOrgId AS Carrier&#xD;&#xA;,ld.PropertyValue AS Loadid&#xD;&#xA;,l.LoadNbr AS BOL&#xD;&#xA;,l.PkupDate&#xD;&#xA;,l.DelvDate&#xD;&#xA;,l.FleetScac AS SCAC&#xD;&#xA;,l.SubFleetScac AS SubScac&#xD;&#xA;,l.ownerorgid AS OwnerOrgId&#xD;&#xA;,l.ProNbr&#xD;&#xA;,o.OwnerName&#xD;&#xA;,l.ShipperPkupNbr&#xD;&#xA;,l.MovementType&#xD;&#xA;INTO #CSLoadNbr&#xD;&#xA;&#xD;&#xA;FROM TOPSAnalyst.dbo.PODCrossServerGDS ld WITH (NOLOCK)&#xD;&#xA;INNER JOIN TOPS.dbo.loads l WITH (NOLOCK)&#xD;&#xA;ON ld.PropertyValue = l.LoadNbr&#xD;&#xA;INNER JOIN TOPS.dbo.Owner o WITH (NOLOCK)&#xD;&#xA;ON l.OwnerOrgId = o.OwnerId&#xD;&#xA;WHERE ld.PropertyName = 'Load Number'&#xD;&#xA;--AND CAST(ld.LastChgDtTm AS DATE) = DATEADD(day,-1,CAST(GETDATE() AS DATE))&#xD;&#xA;;&#xD;&#xA;&#xD;&#xA;--Cross Server for Load Nbr&#xD;&#xA;select &#xD;&#xA;ld.PropertyValue&#xD;&#xA;,Count(ld.PropertyValue) AS DocCount&#xD;&#xA;Into #docCountLdnbr&#xD;&#xA;from  TOPSAnalyst.dbo.PODCrossServerGDS ld WITH(NOLOCK)&#xD;&#xA;inner join #CSLoadNbr p&#xD;&#xA;ON p.BOL = ld.PropertyValue&#xD;&#xA;where ld.PropertyName = 'Load Number'&#xD;&#xA;and p.BOL is not null&#xD;&#xA;group by ld.PropertyValue&#xD;&#xA;;&#xD;&#xA;&#xD;&#xA;--Cross Server for LoadId&#xD;&#xA;select &#xD;&#xA;ld.PropertyValue&#xD;&#xA;,Count(ld.PropertyValue) AS DocCount&#xD;&#xA;Into #docCountLdId&#xD;&#xA;from  TOPSAnalyst.dbo.PODCrossServerGDS ld WITH(NOLOCK)&#xD;&#xA;inner join #LoadDocCS p&#xD;&#xA;ON p.Loadid = ld.PropertyValue&#xD;&#xA;where ld.PropertyName IN ('Billing Load Id','KBX TM Mobile Load Id','Reference Load Id','Load ID')&#xD;&#xA;and p.Loadid is not null&#xD;&#xA;group by ld.PropertyValue&#xD;&#xA;;&#xD;&#xA;&#xD;&#xA;select &#xD;&#xA;ld.LoadId&#xD;&#xA;,Count(ld.LoadId) AS DocCount&#xD;&#xA;Into #docCount&#xD;&#xA;from TOPS.dbo.LoadDocuments ld WITH(NOLOCK)&#xD;&#xA;inner join #LoadDoc p&#xD;&#xA;ON p.loadid = ld.loadid&#xD;&#xA;where ld.DocType = 'PROOF OF DELIVERY'&#xD;&#xA;and p.LoadId is not null&#xD;&#xA;group by ld.LoadId&#xD;&#xA;;&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;SELECT DISTINCT top 1000&#xD;&#xA;p.Carrier&#xD;&#xA;,p.loadid&#xD;&#xA;,p.BOL&#xD;&#xA;,p.PkupDate&#xD;&#xA;,p.DelvDate&#xD;&#xA;,p.SCAC&#xD;&#xA;,p.SubScac&#xD;&#xA;,p.OwnerOrgId&#xD;&#xA;,p.ProNbr&#xD;&#xA;,p.OwnerName&#xD;&#xA;,p.ShipperPkupNbr&#xD;&#xA;,p.MovementType&#xD;&#xA;,CASE WHEN o.PortalPath is null AND dcc.DocCount = 1 THEN 'D:\Users\KBXLBOT5\OneDrive - kochind.com\PODs\PODStaging' &#xD;&#xA;when o.PortalPath is not null and dcc.DocCount &gt; 1 THEN 'D:\Users\KBXLBOT5\OneDrive - kochind.com\PODs\POD Consolidation'&#xD;&#xA;WHEN o.PortalPath is null AND dcc.DocCount &gt; 1 THEN 'D:\Users\KBXLBOT5\OneDrive - kochind.com\PODs\POD Consolidation'&#xD;&#xA;ELSE o.PortalPath END AS PortalPath&#xD;&#xA;,case WHEN p.OwnerOrgID IN ('GYPLS','GYPSM','BPGYP3','GPGYPSUM','GPGYPDED') AND p.MovementType &lt;&gt; 'INBOUND' THEN 'BUILD' &#xD;&#xA;ELSE 'NO' END AS ARRequest&#xD;&#xA;&#xD;&#xA;into #loadreq&#xD;&#xA;&#xD;&#xA;FROM #LoadDoc p&#xD;&#xA;LEFT JOIN #Loads l&#xD;&#xA;ON p.loadid = l.loadid&#xD;&#xA;INNER JOIN TOPSAnalyst.dbo.PODOwner o WITH(NOLOCK)&#xD;&#xA;ON p.OwnerOrgId = o.OwnerId&#xD;&#xA;LEFT JOIN #docCount dcc&#xD;&#xA;ON p.LoadId = dcc.LoadId&#xD;&#xA;where l.LoadId is null&#xD;&#xA;;&#xD;&#xA;&#xD;&#xA;drop table #LoadDoc;&#xD;&#xA;drop table #docCount;&#xD;&#xA;&#xD;&#xA;SELECT DISTINCT&#xD;&#xA;p.Carrier&#xD;&#xA;,p.loadid&#xD;&#xA;,p.BOL&#xD;&#xA;,p.PkupDate&#xD;&#xA;,p.DelvDate&#xD;&#xA;,p.SCAC&#xD;&#xA;,p.SubScac&#xD;&#xA;,p.OwnerOrgId&#xD;&#xA;,p.ProNbr&#xD;&#xA;,p.OwnerName&#xD;&#xA;,p.ShipperPkupNbr&#xD;&#xA;,p.MovementType&#xD;&#xA;,CASE WHEN o.PortalPath is null AND dcc.DocCount = 1 THEN 'D:\Users\KBXLBOT5\OneDrive - kochind.com\PODs\PODStaging' &#xD;&#xA;when o.PortalPath is not null and dcc.DocCount &gt; 1 THEN 'D:\Users\KBXLBOT5\OneDrive - kochind.com\PODs\POD Consolidation'&#xD;&#xA;WHEN o.PortalPath is null AND dcc.DocCount &gt; 1 THEN 'D:\Users\KBXLBOT5\OneDrive - kochind.com\PODs\POD Consolidation'&#xD;&#xA;ELSE o.PortalPath END AS PortalPath&#xD;&#xA;,case WHEN p.OwnerOrgID IN ('GYPLS','GYPSM','BPGYP3','GPGYPSUM','GPGYPDED') AND p.MovementType &lt;&gt; 'INBOUND' THEN 'BUILD' &#xD;&#xA;ELSE 'NO' END AS ARRequest&#xD;&#xA;&#xD;&#xA;INTO #loadreqLoadid&#xD;&#xA;&#xD;&#xA;FROM #LoadDocCS p&#xD;&#xA;LEFT JOIN #Loads l&#xD;&#xA;ON p.loadid = l.loadid&#xD;&#xA;INNER JOIN TOPSAnalyst.dbo.PODOwner o WITH(NOLOCK)&#xD;&#xA;ON p.OwnerOrgId = o.OwnerId&#xD;&#xA;LEFT JOIN #docCountLdId dcc&#xD;&#xA;ON p.LoadId = dcc.PropertyValue&#xD;&#xA;where l.LoadId is null&#xD;&#xA;;&#xD;&#xA;&#xD;&#xA;DROP TABLE #LoadDocCS;&#xD;&#xA;DROP TABLE #docCountLdId;&#xD;&#xA;&#xD;&#xA;SELECT DISTINCT&#xD;&#xA;p.Carrier&#xD;&#xA;,p.loadid&#xD;&#xA;,p.BOL&#xD;&#xA;,p.PkupDate&#xD;&#xA;,p.DelvDate&#xD;&#xA;,p.SCAC&#xD;&#xA;,p.SubScac&#xD;&#xA;,p.OwnerOrgId&#xD;&#xA;,p.ProNbr&#xD;&#xA;,p.OwnerName&#xD;&#xA;,p.ShipperPkupNbr&#xD;&#xA;,p.MovementType&#xD;&#xA;,CASE WHEN o.PortalPath is null AND dcc.DocCount = 1 THEN 'D:\Users\KBXLBOT5\OneDrive - kochind.com\PODs\PODStaging' &#xD;&#xA;when o.PortalPath is not null and dcc.DocCount &gt; 1 THEN 'D:\Users\KBXLBOT5\OneDrive - kochind.com\PODs\POD Consolidation'&#xD;&#xA;WHEN o.PortalPath is null AND dcc.DocCount &gt; 1 THEN 'D:\Users\KBXLBOT5\OneDrive - kochind.com\PODs\POD Consolidation'&#xD;&#xA;ELSE o.PortalPath END AS PortalPath&#xD;&#xA;,case WHEN p.OwnerOrgID IN ('GYPLS','GYPSM','BPGYP3','GPGYPSUM','GPGYPDED') AND p.MovementType &lt;&gt; 'INBOUND' THEN 'BUILD' &#xD;&#xA;ELSE 'NO' END AS ARRequest&#xD;&#xA;&#xD;&#xA;INTO #loadreqLoadNbr&#xD;&#xA;&#xD;&#xA;FROM #CSLoadNbr p&#xD;&#xA;LEFT JOIN #Loads l&#xD;&#xA;ON p.BOL = l.BOL&#xD;&#xA;INNER JOIN TOPSAnalyst.dbo.PODOwner o WITH(NOLOCK)&#xD;&#xA;ON p.OwnerOrgId = o.OwnerId&#xD;&#xA;LEFT JOIN #docCountLdnbr dcc&#xD;&#xA;ON p.BOL = dcc.PropertyValue&#xD;&#xA;where l.LoadId is null&#xD;&#xA;;&#xD;&#xA;&#xD;&#xA;--Moved these up from the bottom for efficiency &#xD;&#xA;drop table #Loads;&#xD;&#xA;DROP TABLE #CSLoadNbr;&#xD;&#xA;DROP TABLE #docCountLdnbr;&#xD;&#xA;&#xD;&#xA;--===============================================================&#xD;&#xA;--Start of Union one table&#xD;&#xA;--===============================================================&#xD;&#xA;&#xD;&#xA;Select distinct&#xD;&#xA;lr.LoadId&#xD;&#xA;,1 as ARPendingPOD&#xD;&#xA;Into #PendingAR&#xD;&#xA;from #LoadReq LR&#xD;&#xA;left join tops.dbo.AROrderLoad OL WITH(NOLOCK)&#xD;&#xA;on ol.LoadId = lr.LoadId&#xD;&#xA;left join Tops.dbo.archarges AR WITH(NOLOCK)&#xD;&#xA;on ol.CustOrderId = ar.CustOrderId&#xD;&#xA;left join tops.dbo.AROrder AO WITH(NOLOCK)&#xD;&#xA;on ol.CustOrderId = ao.CustOrderId&#xD;&#xA;where (ar.ChargeStatus = 'PENDING'&#xD;&#xA;OR ao.PricingStatus = 'Rating Failed')&#xD;&#xA;;&#xD;&#xA;&#xD;&#xA;Selec
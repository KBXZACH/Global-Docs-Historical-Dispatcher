    <ui:ExecuteQuery ContinueOnError="{x:Null}" CommandType="Text" DataTable="[DataTableOutput]" DisplayName="Run query" ExistingDbConnection="[SQL]" sap:VirtualizedContainerService.HintSize="484,88" sap2010:WorkflowViewState.IdRef="ExecuteQuery_1" Sql="--&#xD;&#xA;--&#xD;&#xA;-- 10/20/22            Landon        Created&#xD;&#xA;-- 10/26/22            Landon        Added Portal Path&#xD;&#xA;-- 12/29/22               Landon         Added POD Consolidation Path logic&#xD;&#xA;--05/02/23&#x9;            Tiffany&#x9;&#x9;&#x9;Changed date to open up to pull in anything newer than 04/01/22; changed to sort DESC based off pick up date&#xD;&#xA;--07/06/2023&#x9;&#x9;Tiffany/Raelynn&#x9;adding code to sort by AR Pending, Build products, then delv date&#xD;&#xA; &#xD;&#xA;&#xD;&#xA;DROP TABLE IF EXISTS #Loads&#xD;&#xA;DROP TABLE IF EXISTS #LoadDoc&#xD;&#xA;&#xD;&#xA; &#xD;&#xA;&#xD;&#xA;select Distinct&#xD;&#xA;l.ActualCarrierOrgId AS Carrier&#xD;&#xA;,l.loadid&#xD;&#xA;,l.LoadNbr AS BOL&#xD;&#xA;,l.PkupDate&#xD;&#xA;,l.DelvDate&#xD;&#xA;,l.ActualFleetScac AS SCAC&#xD;&#xA;,l.ActualSubFleetScac AS SubScac&#xD;&#xA;,l.ownerorgid AS OwnerOrgId&#xD;&#xA;,l.ProNbr&#xD;&#xA;,o.OwnerName&#xD;&#xA;,l.ShipperPkupNbr&#xD;&#xA;INTO #Loads&#xD;&#xA;from TOPSAnalyst.dbo.POD p&#xD;&#xA;INNER JOIN TOPS.dbo.loads l WITH (NOLOCK)&#xD;&#xA;ON p.LoadNbr = l.LoadNbr&#xD;&#xA;INNER JOIN TOPS.dbo.Owner o WITH (NOLOCK)&#xD;&#xA;ON l.OwnerOrgId = o.OwnerId&#xD;&#xA;WHERE l.LoadVisibleCd NOT IN ('-15','-16','-17')&#xD;&#xA;AND l.LoadStatusColor &lt;&gt; 'RED'&#xD;&#xA;AND l.LoadId NOT LIKE'%--%'&#xD;&#xA;&#xD;&#xA; &#xD;&#xA;&#xD;&#xA;&#xD;&#xA;SELECT DISTINCT&#xD;&#xA;l.ActualCarrierOrgId AS Carrier&#xD;&#xA;,l.loadid&#xD;&#xA;,l.LoadNbr AS BOL&#xD;&#xA;,l.PkupDate&#xD;&#xA;,l.DelvDate&#xD;&#xA;,l.ActualFleetScac AS SCAC&#xD;&#xA;,l.ActualSubFleetScac AS SubScac&#xD;&#xA;,l.ownerorgid AS OwnerOrgId&#xD;&#xA;,l.ProNbr&#xD;&#xA;,o.OwnerName&#xD;&#xA;,l.ShipperPkupNbr&#xD;&#xA;,l.MovementType&#xD;&#xA;INTO #LoadDoc&#xD;&#xA;FROM TOPS.dbo.LoadDocuments ld WITH(NOLOCK)&#xD;&#xA;INNER JOIN TOPS.dbo.loads l WITH (NOLOCK)&#xD;&#xA;ON ld.loadid = l.Loadid&#xD;&#xA;INNER JOIN TOPS.dbo.Owner o WITH (NOLOCK)&#xD;&#xA;ON l.OwnerOrgId = o.OwnerId&#xD;&#xA;&#xD;&#xA; &#xD;&#xA;&#xD;&#xA;where ld.DocType = 'PROOF OF DELIVERY'&#xD;&#xA;--AND CAST(ld.CreateDtTm AS DATE) = DATEADD(day,-1,CAST(GETDATE() AS DATE))&#xD;&#xA;&#xD;&#xA;--AND CAST(ld.CreateDtTm AS DATE) &lt;= '2022-10-24' --Backfill queue &#xD;&#xA;AND CAST(ld.CreateDtTm AS DATE) &gt;= '2022-04-01' &#xD;&#xA;&#xD;&#xA;&#xD;&#xA;select top 10&#xD;&#xA;ld.LoadId&#xD;&#xA;,Count(ld.LoadId) AS DocCount&#xD;&#xA;Into #docCount&#xD;&#xA;from TOPS.dbo.LoadDocuments ld WITH(NOLOCK)&#xD;&#xA;inner join #LoadDoc p&#xD;&#xA;ON p.loadid = ld.loadid&#xD;&#xA;where ld.DocType = 'PROOF OF DELIVERY'&#xD;&#xA;and p.LoadId is not null&#xD;&#xA;&#xD;&#xA; &#xD;&#xA;&#xD;&#xA;group by ld.LoadId&#xD;&#xA;&#xD;&#xA; &#xD;&#xA;&#xD;&#xA;SELECT DISTINCT Top 1000&#xD;&#xA;p.*&#xD;&#xA;,CASE WHEN o.PortalPath is null AND dcc.DocCount = 1 THEN 'D:\Users\KBXLBOT5\kochind.com\KBXL Analytics - PODs\PODStaging' &#xD;&#xA;when o.PortalPath is not null and dcc.DocCount &gt; 1 THEN 'D:\Users\KBXLBOT5\kochind.com\KBXL Analytics - PODs\POD Consolidation'&#xD;&#xA;WHEN o.PortalPath is null AND dcc.DocCount &gt; 1 THEN 'D:\Users\KBXLBOT5\kochind.com\KBXL Analytics - PODs\POD Consolidation'&#xD;&#xA;ELSE o.PortalPath END AS PortalPath&#xD;&#xA;,case WHEN p.OwnerOrgID IN ('GYPLS','GYPSM','BPGYP3','GPGYPSUM','GPGYPDED') AND p.MovementType &lt;&gt; 'INBOUND' THEN 'BUILD' &#xD;&#xA;ELSE 'NO' END AS ARRequest&#xD;&#xA;into #Loadreq &#xD;&#xA; &#xD;&#xA;&#xD;&#xA;FROM #LoadDoc p&#xD;&#xA;LEFT JOIN #Loads l&#xD;&#xA;ON p.loadid = l.loadid&#xD;&#xA;INNER JOIN TOPSAnalyst.dbo.PODOwner o&#xD;&#xA;ON p.OwnerOrgId = o.OwnerId&#xD;&#xA;LEFT JOIN #docCount dcc&#xD;&#xA;ON p.LoadId = dcc.LoadId&#xD;&#xA;where l.LoadId is null&#xD;&#xA;order by p.PkupDate DESC&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;Select distinct&#xD;&#xA;lr.LoadId&#xD;&#xA;,1 as ARPendingPOD&#xD;&#xA;Into #PendingAR&#xD;&#xA;from #LoadReq LR&#xD;&#xA;left join tops.dbo.AROrderLoad OL&#xD;&#xA;on ol.LoadId = lr.LoadId&#xD;&#xA;left join Tops.dbo.archarges AR&#xD;&#xA;on ol.CustOrderId = ar.CustOrderId&#xD;&#xA;left join tops.dbo.AROrder AO&#xD;&#xA;on ol.CustOrderId = ao.CustOrderId&#xD;&#xA;where (ar.ChargeStatus = 'PENDING'&#xD;&#xA;OR ao.PricingStatus = 'Rating Failed')&#xD;&#xA;&#xD;&#xA;Select &#xD;&#xA;lr.*&#xD;&#xA;,par.ARPendingPOD&#xD;&#xA;From #LoadReq lr&#xD;&#xA;left join #PendingAR PAR&#xD;&#xA;on lr.LoadId = par.LoadId&#xD;&#xA;order by ARPendingPOD desc, lr.ARRequest asc, lr.DelvDate asc&#xD;&#xA;&#xD;&#xA;Drop Table #LoadReq&#xD;&#xA;Drop Table #PendingAR&#xD;&#xA;&#xD;&#xA; &#xD;&#xA;&#xD;&#xA;&#xD;&#xA;drop table #Loads;&#xD;&#xA;drop table #LoadDoc;&#xD;&#xA;drop table #docCount;&#xD;&#xA;" TimeoutMS="1800000">
